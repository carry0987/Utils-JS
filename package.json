{
  "name": "@carry0987/utils",
  "version": "3.10.3",
  "description": "A library for using common tools and alias methods",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": "./dist/types/index.d.ts",
    "./interfaces": "./dist/interfaces/index.d.ts"
  },
  "sideEffects": false,
  "scripts": {
    "lint": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "build:js": "rollup --config rollup.config.ts --configPlugin typescript --environment BUILD:js",
    "build:dts": "rollup --config rollup.config.ts --configPlugin typescript --environment BUILD:dts",
    "build": "npm run build:js && npm run build:dts",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carry0987/Utils-JS.git"
  },
  "keywords": [
    "util",
    "utils"
  ],
  "author": "carry0987",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carry0987/Utils-JS/issues"
  },
  "homepage": "https://github.com/carry0987/Utils-JS#readme",
  "devDependencies": {
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^25.0.10",
    "@vitest/coverage-v8": "4.0.18",
    "happy-dom": "^20.3.7",
    "msw": "^2.12.7",
    "prettier": "^3.8.1",
    "rollup": "^4.56.0",
    "rollup-plugin-dts": "^6.3.0",
    "rollup-plugin-tsconfig-paths": "^1.5.2",
    "tslib": "^2.8.1",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "msw"
    ]
  }
}
